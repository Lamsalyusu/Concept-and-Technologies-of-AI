   "source": [
    "def sum_nested_list(nested_list):\n",
    "\n",
    "    total = 0\n",
    "    for element in nested_list:\n",
    "        if isinstance(element, list):\n",
    "            total += sum_nested_list(element)\n",
    "        else:\n",
    "            total += element\n",
    "    return total \n",
    "print(sum_nested_list([1, [2, [3, 4], 5], 6, [7,8]]))"
   ]
  }
